<div class="rigor-builder mt-5">
    <div class="w-100" sdlms-component="rigor-builder">
        <div class="sdlms-section">
            <div
                class="sdlms-section-header text-center sdlms-primary-bg cursor-pointer custom-padding-x-40 primary-header font-weight-500 sdlms-text-white-20px">
                Rigor Builder
            </div>
            <div class="sdlms-section-body px-3 pt-4">
                <div class="px-5">
                    <div class="w-100 mt-3 mb-5" collpsible="">
                        <h2 class="text-center text-capitalize"><br /></h2>
                        <div class="problem-statement bg-gray p-5 sdlms-section shadow-none">
                            <p class="mb-4">Before you submit your solutions, please reflect on your
                                experience.</p>
                            <textarea class="border-0 d-block p-2 m-0 sdlms-asset-content-body sdlms-section w-100"
                                style="resize: none" id="reflection"></textarea>
                        </div>
                    </div>
                    <!-- icons -->
                    <div class="p-5 ">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-log-out float-right cursor-pointer">
                            <path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path>
                            <polyline points="16 17 21 12 16 7"></polyline>
                            <line x1="21" y1="12" x2="9" y2="12"></line>
                        </svg>


                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-eye d-none mr-4 float-right mr-3 cursor-pointer" preview-icon>
                            <path d="M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z"></path>
                            <circle cx="12" cy="12" r="3"></circle>
                        </svg>

                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-fast-forward  d-none mr-4 float-right cursor-pointer" next-icon>
                            <polygon points="13 19 22 12 13 5 13 19"></polygon>
                            <polygon points="2 19 11 12 2 5 2 19"></polygon>
                        </svg>
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-send float-right mr-4 cursor-pointer" id="complete" submit-icon>
                            <line x1="22" y1="2" x2="11" y2="13"></line>
                            <polygon points="22 2 15 22 11 13 2 9 22 2"></polygon>
                        </svg>

                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-save float-right mr-4 cursor-pointer" id="save">
                            <path d="M19 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11l5 5v11a2 2 0 0 1-2 2z"></path>
                            <polyline points="17 21 17 13 7 13 7 21"></polyline>
                            <polyline points="7 3 7 8 15 8"></polyline>
                        </svg>

                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                            class="feather feather-edit float-right mr-4 cursor-pointer">
                            <path d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7"></path>
                            <path d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z"></path>
                        </svg>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>